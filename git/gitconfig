[alias]
    a = add
    aa = add .
    b = branch -vv
    bb = branch -vv --all
    bd = branch -D
    c = commit
    ca = commit -a
    cam = commit -a -m
    cl = clone
    cm = commit -m
    co = checkout
    cob = checkout -b
    d = diff
    i = init
    l = log --all --graph --decorate --oneline -n 30
    lo = log --all --graph --decorate --oneline -n 30
    lof = log --all --graph --decorate --oneline
    m = merge
    mt = mergetool
    pl = pull
    plo = pull origin
    pu = push
    puo = push origin
    puom = push origin main
    r = remote
    ra = remote add
    s = status -s
    sa = submodule add
    st = stash
    stp = stash pop
    uc = reset HEAD^ --soft
    update-submodules = submodule update --init --recursive
    upgrade-submodules = submodule update --init --remote
    ups = push -u origin HEAD
[commit]
    template = ~/.gitmessage
    verbose = true
[core]
    autocrlf = input
    editor = /usr/local/bin/nvim
    excludesfile = ~/.gitignore
    pager = less -R
[credential]
    helper = osxkeychain
[filter "lfs"]
    clean = git-lfs clean -- %f
    process = git-lfs filter-process
    required = true
    smudge = git-lfs smudge -- %f
[merge]
    defaultToUpstream = true
    tool = vimdiff
[mergetool "vimdiff"]
    cmd = vimdiff -f $LOCAL $MERGED $REMOTE
[pull]
    ff = only
    rebase = true
[rebase]
    autoStash = true
[reviewboard]
    url = http://reviewboard.mathworks.com/
[log]
    excludeDecoration = *origin/j*
    excludeDecoration = *tag: j*
[user]
    name = gumamahe
    email = gumamahe@mathworks.com
[help]
    autocorrect = 10
